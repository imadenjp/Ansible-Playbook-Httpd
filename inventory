# Menambahkan group host
[srv1]
srv1 ansible_host=20.20.20.10

[srv2]
srv2 ansible_host=20.20.20.20

# Menambahkan variable pada setiap group
[srv1:vars]
ansible_user=user
ansible_sudo_pass=user
ansible_ssh_private_key_file=~/.ssh/id_rsa

[srv2:vars]
ansible_user=user
ansible_sudo_pass=user
ansible_ssh_private_key_file=~/.ssh/id_rsa